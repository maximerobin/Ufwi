zone "my.edenwall.com" {
    type master;
    file "my.edenwall.com.db";
};


{% for fdomain in forwarded_domains %}
zone "{{ fdomain }}" {
    type forward;
    forwarders { {% for ip in forwarded_domains[fdomain]  %} {{ip}};{% endfor %} };
};

{% endfor %}
