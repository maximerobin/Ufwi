
################
# mangle table #
################
*mangle
:PREROUTING ACCEPT [0:0]
:INPUT ACCEPT [0:0]
:FORWARD ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
:POSTROUTING ACCEPT [0:0]

-A PREROUTING -j CONNMARK --restore-mark
-A POSTROUTING -m mark ! --mark 0 -j CONNMARK --save-mark --mask 0x0000ffff

COMMIT

################
# filter table #
################
*filter
:INPUT DROP [0:0]
:FORWARD DROP [0:0]
:OUTPUT ACCEPT [0:0]
:ETH0-ETH1 - [0:0]
:ETH0-ETH2 - [0:0]
:ETH1-ETH0 - [0:0]
:ETH1-ETH2 - [0:0]
:ETH1-ETH3 - [0:0]
:ETH2-ETH1 - [0:0]
:ETH3-ETH0 - [0:0]
:TUNP-ETH1 - [0:0]

# Default filter rules
# --------------------
-A FORWARD -m state --state RELATED,ESTABLISHED -j ACCEPT
-A INPUT -m state --state RELATED,ESTABLISHED -j ACCEPT
-A OUTPUT -m state --state RELATED,ESTABLISHED -j ACCEPT
-A FORWARD -m state --state INVALID -j ULOG --ulog-prefix "Drop INVALID FORWARD"
-A FORWARD -m state --state INVALID -j DROP
-A OUTPUT -m state --state INVALID -j ULOG --ulog-prefix "Drop INVALID OUTPUT"
-A OUTPUT -m state --state INVALID -j DROP
-A INPUT -i lo -j ACCEPT -m comment --comment "Trust loopback"
-A OUTPUT -o lo -j ACCEPT -m comment --comment "Trust loopback"


# Dispatch FORWARD to the different chains
# ----------------------------------------
-A FORWARD -i eth0 -o eth1 -j ETH0-ETH1
-A FORWARD -i eth0 -o eth2 -j ETH0-ETH2
-A FORWARD -i eth1 -o eth0 -j ETH1-ETH0
-A FORWARD -i eth1 -o eth2 -j ETH1-ETH2
-A FORWARD -i eth1 -o eth3 -j ETH1-ETH3
-A FORWARD -i eth2 -o eth1 -j ETH2-ETH1
-A FORWARD -i eth3 -o eth0 -j ETH3-ETH0
-A FORWARD -i tunp -o eth1 -j TUNP-ETH1

