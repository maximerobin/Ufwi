
################
# mangle table #
################
*mangle
:PREROUTING ACCEPT [0:0]
:INPUT ACCEPT [0:0]
:FORWARD ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
:POSTROUTING ACCEPT [0:0]

-A PREROUTING -j CONNMARK --restore-mark
-A POSTROUTING -m mark ! --mark 0 -j CONNMARK --save-mark --mask 0x0000ffff

COMMIT

################
# filter table #
################
*filter
:INPUT DROP [0:0]
:FORWARD DROP [0:0]
:OUTPUT ACCEPT [0:0]
:BOND0.1-BOND0.11 - [0:0]
:BOND0.1-BOND0.12 - [0:0]
:BOND0.1-BOND0.301 - [0:0]
:BOND0.1-BOND0.510 - [0:0]
:BOND0.11-BOND0.510 - [0:0]
:BOND0.12-BOND0.510 - [0:0]
:BOND0.301-BOND0.12 - [0:0]
:BOND0.301-BOND0.510 - [0:0]
:BOND0.510-BOND0.1 - [0:0]
:BOND0.510-BOND0.12 - [0:0]
:BOND0.510-BOND0.301 - [0:0]

# Default filter rules
# --------------------
-A FORWARD -m state --state RELATED,ESTABLISHED -j ACCEPT
-A INPUT -m state --state RELATED,ESTABLISHED -j ACCEPT
-A OUTPUT -m state --state RELATED,ESTABLISHED -j ACCEPT
-A FORWARD -m state --state INVALID -j ULOG --ulog-prefix "Drop INVALID FORWARD"
-A FORWARD -m state --state INVALID -j DROP
-A OUTPUT -m state --state INVALID -j ULOG --ulog-prefix "Drop INVALID OUTPUT"
-A OUTPUT -m state --state INVALID -j DROP
-A INPUT -i lo -j ACCEPT -m comment --comment "Trust loopback"
-A OUTPUT -o lo -j ACCEPT -m comment --comment "Trust loopback"


# Dispatch FORWARD to the different chains
# ----------------------------------------
-A FORWARD -i bond0.1 -o bond0.11 -j BOND0.1-BOND0.11
-A FORWARD -i bond0.1 -o bond0.12 -j BOND0.1-BOND0.12
-A FORWARD -i bond0.1 -o bond0.301 -j BOND0.1-BOND0.301
-A FORWARD -i bond0.1 -o bond0.510 -j BOND0.1-BOND0.510
-A FORWARD -i bond0.11 -o bond0.510 -j BOND0.11-BOND0.510
-A FORWARD -i bond0.12 -o bond0.510 -j BOND0.12-BOND0.510
-A FORWARD -i bond0.301 -o bond0.12 -j BOND0.301-BOND0.12
-A FORWARD -i bond0.301 -o bond0.510 -j BOND0.301-BOND0.510
-A FORWARD -i bond0.510 -o bond0.1 -j BOND0.510-BOND0.1
-A FORWARD -i bond0.510 -o bond0.12 -j BOND0.510-BOND0.12
-A FORWARD -i bond0.510 -o bond0.301 -j BOND0.510-BOND0.301

