-A BOND0.1-BOND0.12 -s 10.30.0.0/16 -d 10.0.0.0/8 -p udp --sport 1:65535 --dport 123 -m state --state NEW -j ACCEPT -m comment --comment "IPv4 rule #590: rule 2"

# IPv4 rule #190
# --------------
# Bind9 vers DSTI
-A BOND0.510-BOND0.12 -s 10.30.15.0/24 -p tcp --sport 1024:65535 --dport 53 --syn -m state --state NEW -j ACCEPT -m comment --comment "IPv4 rule #190: rule 1"
-A BOND0.510-BOND0.12 -s 10.30.15.0/24 -p udp --sport 1024:65535 --dport 53 -m state --state NEW -j ACCEPT -m comment --comment "IPv4 rule #190: rule 2"

# IPv4 rule #540
# --------------
# Microsoft Exchange Server 5.5
-A BOND0.510-BOND0.12 -s 75.0.30.184 -d 10.0.0.0/8 -p tcp --sport 1024:65535 --dport 25 --syn -m state --state NEW -j ACCEPT -m comment --comment "IPv4 rule #540: rule 1"
-A BOND0.510-BOND0.12 -s 75.0.30.184 -d 10.0.0.0/8 -p tcp --sport 1024:65535 --dport 102 --syn -m state --state NEW -j ACCEPT -m comment --comment "IPv4 rule #540: rule 2"

# IPv4 rule #600
# --------------
# NTP server depuis serveurs
-A BOND0.510-BOND0.12 -d 10.0.0.0/8 -p tcp --sport 1024:65535 --dport 119 --syn -m state --state NEW -j ACCEPT -m comment --comment "IPv4 rule #600: rule 1"
-A BOND0.510-BOND0.12 -d 10.0.0.0/8 -p udp --sport 1:65535 --dport 123 -m state --state NEW -j ACCEPT -m comment --comment "IPv4 rule #600: rule 2"

# IPv4 rule #810
# --------------
# Proxy DSTI (depuis Serveurs)
-A BOND0.510-BOND0.12 -s 10.30.15.0/24 -d 10.160.26.10 -p tcp --sport 1024:65535 --dport 80 --syn -m state --state NEW -j ACCEPT -m comment --comment "IPv4 rule #810: rule 1"


# Default decisions
# -----------------
-A BOND0.1-BOND0.11 -j ULOG --ulog-prefix "F0d:Default DROP for BOND0.1-BO"
-A BOND0.1-BOND0.11 -j DROP -m comment --comment "Default decision"
-A BOND0.1-BOND0.12 -j ULOG --ulog-prefix "F0d:Default DROP for BOND0.1-BO"
-A BOND0.1-BOND0.12 -j DROP -m comment --comment "Default decision"
-A BOND0.1-BOND0.301 -j ULOG --ulog-prefix "F0d:Default DROP for BOND0.1-BO"
-A BOND0.1-BOND0.301 -j DROP -m comment --comment "Default decision"
-A BOND0.1-BOND0.510 -j ULOG --ulog-prefix "F0d:Default DROP for BOND0.1-BO"
-A BOND0.1-BOND0.510 -j DROP -m comment --comment "Default decision"
-A BOND0.11-BOND0.510 -j ULOG --ulog-prefix "F0a:Default ACCEPT for BOND0.11"
-A BOND0.11-BOND0.510 -j ACCEPT -m comment --comment "Default decision"
-A BOND0.12-BOND0.510 -j ULOG --ulog-prefix "F0d:Default DROP for BOND0.12-B"
-A BOND0.12-BOND0.510 -j DROP -m comment --comment "Default decision"
-A BOND0.301-BOND0.12 -j ULOG --ulog-prefix "F0d:Default DROP for BOND0.301-"
-A BOND0.301-BOND0.12 -j DROP -m comment --comment "Default decision"
-A BOND0.301-BOND0.510 -j ULOG --ulog-prefix "F0d:Default DROP for BOND0.301-"
-A BOND0.301-BOND0.510 -j DROP -m comment --comment "Default decision"
-A BOND0.510-BOND0.1 -j ULOG --ulog-prefix "F0d:Default DROP for BOND0.510-"
-A BOND0.510-BOND0.1 -j DROP -m comment --comment "Default decision"
-A BOND0.510-BOND0.12 -j ULOG --ulog-prefix "F0d:Default DROP for BOND0.510-"
-A BOND0.510-BOND0.12 -j DROP -m comment --comment "Default decision"
-A BOND0.510-BOND0.301 -j ULOG --ulog-prefix "F0d:Default DROP for BOND0.510-"
-A BOND0.510-BOND0.301 -j DROP -m comment --comment "Default decision"
-A INPUT -j ULOG --ulog-prefix "I0d:Default DROP for INPUT"
-A INPUT -j DROP -m comment --comment "Default decision"
-A FORWARD -j ULOG --ulog-prefix "F0d:Default DROP for FORWARD"
-A FORWARD -j DROP -m comment --comment "Default decision"
-A OUTPUT -j ACCEPT -m comment --comment "Default decision"
COMMIT

